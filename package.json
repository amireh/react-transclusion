{
  "name": "react-transclusion",
  "version": "1.3.2",
  "description": "Render arbitrary components into outlets for use in dynamic layouts.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib --ignore '*.test.js' src",
    "lint": "eslint src",
    "test": "node --harmony --harmony-destructuring ./node_modules/.bin/_mocha -r babel-core/register 'src/**/*.test.js'",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/amireh/react-transclusion.git"
  },
  "keywords": [
    "react",
    "outlet",
    "outlets",
    "layout",
    "templates",
    "transclusion",
    "dynamic"
  ],
  "author": "Ahmad Amireh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amireh/react-transclusion/issues"
  },
  "homepage": "https://github.com/amireh/react-transclusion#readme",
  "devDependencies": {
    "babel-cli": "6.7.7",
    "babel-core": "6.7.7",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "chai": "3.5.0",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "jsdom": "8.4.0",
    "mocha": "2.4.5",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-drill": "2.1.1"
  },
  "dependencies": {
    "invariant": "2.2.1",
    "object-assign": "4.0.1"
  }
}
